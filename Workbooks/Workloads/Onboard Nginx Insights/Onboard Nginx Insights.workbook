                            "items": [
                                {
                                    "type": 1,
                                    "content": {
                                        "json": "Usługa Azure Monitor wymaga identyfikatora URI punktu końcowego statystyki elementu zastępczego NGINX, aby zbierać metryki użycia i inne metryki. "
                                    },
                                    "name": "text - 3"
                                },
